{
  "compilerOptions": {
    "target": "ES2015",
    "module": "commonjs",
    "outDir": "assets/js",
    "sourceMap": true,
    "strict": true,
    "allowJs": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitAny": false,
    "strictNullChecks": false
  },
  "exclude": [],
  "include": ["*"]
}
